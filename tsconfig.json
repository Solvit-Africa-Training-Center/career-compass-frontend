    "paths": {
      "@/*": ["./src/*"],
      "@/components/*": ["./src/components/*"],
      "@ui/*": ["./src/components/ui/*"],
      "@cards/*": ["./src/components/cards/*"],
      "@pages/*":["./src/pages/*"],
    }
  }
}